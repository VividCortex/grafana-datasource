{
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": 1,
    "no-unused-vars": 1,
    "no-unreachable": 1,
    "curly": 1,
    "default-case": 1,
    "eqeqeq": 1,
    "no-redeclare": 1
  }
}
